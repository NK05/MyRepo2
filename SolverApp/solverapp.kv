                    Color:
                        rgb:0,.5,.5
                    Rectangle:
                        size:self.size
                        pos:self.pos  
                left_action_items:[['menu',lambda x: app.navigation_draw()]]
                size_hint_y:None
                size_hint_y:.1
                MDLabel:
                    size_hint_x:None
                    width:50
                MDLabel:
                    text:'Solver'
                    pos_hint: {'x':0.5,'y':0}
                    size_hint_x:None
                    width:200
                MDLabel:
                    text:'Panashe'
                    pos_hint: {'center_x':.5,'center_y':0.5}
                    size_hint_x:None
                    width:200
                MDTextField:
                    hint_text:'SEARCH'

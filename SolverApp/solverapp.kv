Window_Manager:
#################################################################################################################################
    Welcome_Page:
        name:'welcome'
        orientation:'vertical'
        MDLabel:
            canvas.before:
                Color:
                    rgb:.5,.5,1
                Rectangle: 
                    size:self.size
                    pos:self.pos
            font_size: dp(55)
            font_name: 'RESOURCES/fonts/Sackers-Gothic-Std-Light.ttf'
            text:'WELCOME TO SOLVER'
            size_hint_y:None
            size_hint_y:1
        BoxLayout:
            size_hint_y: None
            size_hint_y: .2
            Button:
                font_size: dp(30)
                font_name: 'RESOURCES/fonts/Eurostile.ttf'
                background_color: 1, .3, .4, .85
                text:"About"
                on_release: app.show_about_dialog()
            Button:
                font_size: dp(30)
                font_name: 'RESOURCES/fonts/Eurostile.ttf'
                text:"LogIn"
                background_color: 1, .3, .4, .85
                on_release: root.current = 'Level_Selection'

##################################################################################################################################################################
    
    Login_Page:
        name:'Auth'
        MDToolbar:
            title:'SOLVER LOGIN PAGE'
            pos_hint: {'center_x':0.5,'center_y':.96}
        MDTextField:
            hint_text: 'Enter username'
            #icon_right:'android'
            pos_hint: {'center_x':0.5,'center_y':0.6}
            size_hint_x:None
            width:400
        MDTextField:
            password:True
            hint_text: 'Enter password'
            #icon_right:'android'
            pos_hint: {'center_x':0.5,'center_y':0.5}
            passwords:True
            size_hint_x:None
            width:400
        MDRectangleFlatButton:
            text:'Enter'
            pos_hint:{'center_x':0.4,'center_y':0.4}
            size_hint_x:None
            width:200
            #on_press:app.allow_access()
            on_release: root.current = 'Main'
            on_press:app.inventory_check()

        MDRectangleFlatButton:
            text:'Back'
            pos_hint:{'center_x':0.6,'center_y':0.4}
            size_hint_x:None
            width:200
            #on_press:app.allow_access()
            on_release: root.current = 'Level_Selection'

